{
  "name": "fronend_bot",
  "version": "0.0.1",
  "description": "Fronend helper bot",
  "main": "index.js",
  "scripts": {
    "start": "tsc && cross-env NODE_ENV=production node ./build/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch \"src/**\" --ext \"js,ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/index.ts\"",
    "pretty": "prettier --write ./src",
    "clean": "rimraf build/",
    "lint": "npx eslint -c ./.eslintrc.json ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint -c ./.eslintrc.json --fix ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dezmunt1/fronend_bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dezmunt1/fronend_bot/issues"
  },
  "homepage": "https://github.com/dezmunt1/fronend_bot#readme",
  "dependencies": {
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "jsdom": "^20.0.2",
    "telegraf": "^4.10.0"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
